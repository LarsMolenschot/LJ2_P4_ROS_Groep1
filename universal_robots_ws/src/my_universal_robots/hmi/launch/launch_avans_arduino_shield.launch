<?xml version="1.0"?>
<launch>
    <node name="serial_node_hmi"        pkg="rosserial_python"      type="serial_node.py">
    <param name="port"              type="string"               value="/dev/ttyACM0"/>
    <param name="baud"              type="int"                  value="57600"/>
    </node>

    <node name="serial_node_regelaar"        pkg="rosserial_python"      type="serial_node.py">
    <param name="port"              type="string"               value="/dev/ttyACM1"/>
    <param name="baud"              type="int"                  value="115200"/>
    </node>
</launch>
